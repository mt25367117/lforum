<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.1.dtd">
<urlrewrite>
	<rule>
		<from>^/(\w+).html$</from>
		<to type="forward">$1/$1.action</to>
	</rule>
	<rule>
		<from>^/(\w+)-(\w+).html$</from>
		<to type="forward">$1/$1!$2.action</to>
	</rule>
</urlrewrite>